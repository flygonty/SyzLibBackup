r0 = syz_declare_char_array(0x2000)
r1 = syz_mnl_nlmsg_batch_start(r0, 0x2000)
syz_nftnl_batch_begin(r1, 0x0)
syz_create_table(r1, &AUTO='syz0\x00', 0x1, 0x0)
syz_create_chain(r1, &AUTO='syz0\x00', &AUTO='syz0\x00', 0x1, 0x0)
syz_batch_end(r1, 0x1)
syz_mnl_nlmsg_batch_next(r1)
r4 = syz_mnl_socket_open()
r5 = syz_mnl_socket_get_portid(r4)
syz_sendto(r4, r1)
syz_mnl_nlmsg_batch_stop(r1)
syz_recvfrom_cb(r4, r0, r5, 0x2)
